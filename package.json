{
  "name": "angular4_universal_spa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run clean && webpack --env.aot --env.server && webpack --env.aot --env.client",
    "start": "npm run start:heroku",
    "start:normal": "npm run build && npm run server",
    "start:forever": "forever stop 0 && npm run build && forever start dist/server*.js",
    "build": "webpack --progress --profile --bail",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "nodemon dist/server*.js",
    "watch": "webpack --watch",
    "clean:all": "rimraf dist && rimraf node_modules"
  },
  "dependencies": {
    "@angular-redux/store": "^6.6.0",
    "@angular/animations": "^4.4.7",
    "@angular/common": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/compiler-cli": "^4.4.7",
    "@angular/core": "^4.4.7",
    "@angular/http": "^4.4.0-RC.0",
    "@angular/platform-browser": "^4.4.7",
    "@angular/platform-browser-dynamic": "v4.4.0-RC.0",
    "@angular/platform-server": "^4.4.7",
    "@angular/router": "^4.4.7",
    "@ngtools/webpack": "^1.10.2",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "@types/express": "^4.17.8",
    "@types/node": "^14.10.3",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^6.1.0",
    "core-js": "^2.6.11",
    "css-loader": "^0.28.11",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "forever": "^0.15.3",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "^2.8.1",
    "json-loader": "^0.5.3",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "npm": "^5.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "2.0.6",
    "raw-loader": "0.5.1",
    "redux": "^3.6.0",
    "redux-logger": "3.0.6",
    "redux-sessionstorage": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.7.1",
    "rxjs": "^5.5.12",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "serialize-javascript": "^1.9.1",
    "shelljs": "^0.7.0",
    "style-loader": "0.18.2",
    "ts-helpers": "^1.1.1",
    "tslib": "^1.13.0",
    "typescript": "^2.9.2",
    "url-loader": "^0.5.6",
    "web-animations-js": "^2.3.2",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {},
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.1"
  },
  "description": "Angular4 Universal project",
  "main": "karma-shim.js",
  "repository": {
    "type": "git",
