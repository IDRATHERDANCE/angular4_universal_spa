{
  "name": "angular-ssr",
  "version": "16.1.1",
  "scripts": {
    "install:memory": "node --max-old-space-size=8000 $(which npm) install",
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "coverage": "ng test --no-watch --code-coverage",
    "serve": "node server",
    "lint": "ng lint",
    "dev:ssr": "ng run ana-rajcevic-portfolio:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr:memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run build && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run ana-rajcevic-portfolio:server",
    "build:ssr": "ng build && ng run ana-rajcevic-portfolio:server",
    "prerender": "ng run ana-rajcevic-portfolio:prerender && cp .htaccess dist/browser/",
    "prerender:memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng run ana-rajcevic-portfolio:prerender && cp .htaccess dist/browser/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.1",
    "@angular/common": "16.1.1",
    "@angular/compiler": "16.1.1",
    "@angular/core": "16.1.1",
    "@angular/forms": "16.1.1",
    "@angular/platform-browser": "16.1.1",
    "@angular/platform-browser-dynamic": "16.1.1",
    "@angular/platform-server": "16.1.1",
    "@angular/router": "16.1.1",
    "@angular/service-worker": "16.1.1",
    "@fortawesome/fontawesome-free": "6.4.0",
    "@nguniversal/express-engine": "16.1.0",
    "bootstrap": "5.3.0",
    "express": "4.18.2",
    "rxjs": "7.8.1",
    "tslib": "2.5.3",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.0",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.1.0",
    "@angular/compiler-cli": "16.1.1",
    "@nguniversal/builders": "16.1.0",
    "@types/express": "4.17.17",
    "@types/jasmine": "4.3.2",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.42.0",
    "jasmine-core": "5.0.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.0.4"
  }
}
